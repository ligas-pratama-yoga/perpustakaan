{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.7.3" }
    },
    "outputDirectory": "public",
    "routes": [
        {
            "src": "/css/(.*)",
            "dest": "/public/$1"
        },
        {
            "src": "/js/(.*)",
            "dest": "/public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}